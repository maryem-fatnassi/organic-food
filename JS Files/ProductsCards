function filterByType(productsArray,type){
    return productsArray.filter( product => product.type === type );
}
const meatsArray = filterByType(allproductsArray,'meat');
const orangesArray = filterByType(allproductsArray,'orange');
const fastFoodArray = filterByType(allproductsArray,'fastFood');
//console.log(meatsArray,orangesArray);

class DisplayFeaturedProducts{
    static products = new Map([
        ['All',allproductsArray],
        ['Meat',meatsArray],
        ['Orange',orangesArray],
        ['Fast Food',fastFoodArray],
    ]);
    static displayProductTitles(){
        const titles = DisplayFeaturedProducts.products.keys();
        Array.from(titles).forEach((title)=>{
            const titleElement = document.createElement('li');
            titleElement.className = 'nav-element';
            titleElement.textContent = title;
            if(title === 'All'){
                titleElement.classList.add('nav-active');
            }
            titleElement.addEventListener('click',()=>{
                const navElement = document.querySelectorAll('.nav-element');
                navElement.forEach(item => item.classList.remove('nav-active'));
                titleElement.classList.add('nav-active');
                DisplayFeaturedProducts.showFeaturedProducts(title);
            })
            navFeaturedProductList.appendChild(titleElement);
        })
    }
    static showFeaturedProducts(category){
        const arrayOfProducts = DisplayFeaturedProducts.products.get(category);
        featuredProducts.innerHTML='';
        console.log(arrayOfProducts)
       arrayOfProducts.forEach(clickedTitle=> clickedTitle.displayFeaturedProducts());
    }
}
DisplayFeaturedProducts.displayProductTitles()    

