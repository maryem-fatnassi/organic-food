    usageCount.set(key , current+1);
    let item = document.getElementById(key);
    if(!item){
     item = document.createElement('h3');
     item.id = key;
     purchasesMenu.appendChild(item)
   }
    item.textContent = `${key} ${price} $ x ${ current }`;
